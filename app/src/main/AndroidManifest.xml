<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.daylight">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.daylight.habits.HabitsActivity"
            android:theme="@style/AppTheme.OverlapSystemBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.daylight.habitdetail.HabitDetailActivity" />
        <activity android:name="com.example.daylight.addedithabit.AddEditHabitActivity" />
    </application>
</manifest>
